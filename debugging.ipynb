   "source": [
    "from typing import List, TypedDict\n",
    "from langchain_core.messages import HumanMessage, BaseMessage\n",
    "from langchain_groq import ChatGroq\n",
    "from langgraph.graph import StateGraph\n",
    "\n",
    "# Define state\n",
    "class State(TypedDict):\n",
    "    messages: List[BaseMessage]\n",
    "\n",
    "# Define LLM\n",
    "llm = ChatGroq(model=\"llama-3.1-8b-instant\")\n",
    "\n",
    "# Node function\n",
    "def llm_with_tool(state: State) -> State:\n",
    "    response = llm.invoke(state[\"messages\"])\n",
    "    return {\"messages\": state[\"messages\"] + [response]}\n",
    "\n",
    "# Build graph\n",
    "builder = StateGraph(State)\n",
    "builder.add_node(\"llm\", llm_with_tool)\n",
    "builder.set_entry_point(\"llm\")\n",
    "\n",
    "graph = builder.compile()\n",
    "\n",
    "# Run\n",
    "result = graph.invoke({\"messages\": [HumanMessage(content=\"what's (3+5) * 12?\")]})\n",
    "print(result[\"messages\"][-1].content)\n"
   ],
   "id": "b0c68ae2dd7bd97a",
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "To solve the equation (3+5) * 12, we need to follow the order of operations (PEMDAS):\n",
      "\n",
      "1. Evaluate the expression inside the parentheses: 3+5 = 8\n",
      "2. Multiply 8 by 12: 8 * 12 = 96\n",
      "\n",
      "So the answer is 96.\n"
     ]
    }
   ],
